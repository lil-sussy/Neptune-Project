{
	"entry": {
		"index": "./src/index.tsx"
	},
	"resolve": {
		"extensions": [".ts", ".tsx", ".js", ".jsx"]
	},
	"module": {
		"rules": [
			{
				"test": "\\.tsx?$",
				"use": "babel-loader"
			},
			{
				"test": "\\.scss$",
				"use": ["style-loader", "css-loader", "sass-loader"]
			}
		]
	},
	"output": {
		"path": "./dist",
		"filename": "bundle.js"
	}
}
