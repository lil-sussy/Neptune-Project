      "args": [
        "runserver"
      ],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/manage.py"
    },
    {
      "name": "Django Test \"text\"",
      "type": "debugpy",
      "request": "launch",
      "args": [
        "test", "text"
      ],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/manage.py"
    },
    {
      "name": "Django Test \"text\" With pre deletion",
      "type": "debugpy",
      "request": "launch",
      "args": [
        "test",
        "text"
      ],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/manage.py",
      "preLaunchTask": "Empty ChromaDB Collections"
    },
  ]
}